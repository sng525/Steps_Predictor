    xmlns:local="clr-namespace:mobile_application"
    xmlns:views="clr-namespace:mobile_application"
    Shell.FlyoutBehavior="Disabled"
    Title="mobile_application">

    <ShellContent
        Title="main"
        ContentTemplate="{DataTemplate views:MainPage}"
        Route="MainPage" />

</Shell>
